<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <!-- interface-requires about_pomidor_dialog 1.0 -->
  <object class="AboutPomidorDialog" id="about_pomidor_dialog">
    <property name="can_focus">False</property>
    <property name="border_width">5</property>
    <property name="resizable">False</property>
    <property name="window_position">center-always</property>
    <property name="icon">../media/pomidor.svg</property>
    <property name="type_hint">normal</property>
    <property name="program_name">Pomidor</property>
    <property name="version">Version 0.2</property>
    <property name="copyright">Copyright (C) 2013 Dieter Konrad dkotrada@gmail.com</property>
    <property name="comments" translatable="yes">Pomodoro Time Manager</property>
    <property name="website">http://launchpad.net/~dkotrada</property>
    <property name="website_label" translatable="yes">Project Page</property>
    <property name="license" translatable="yes"># Copyright (C) 2013 Dieter Konrad dkotrada@gmail.com# This file is distributed under the license LGPL version 2 or later
</property>
    <property name="authors">Copyright (C) 2013 Dieter Konrad dkotrada@gmail.co</property>
    <property name="documenters">Dieter Konrad</property>
    <property name="translator_credits" translatable="no">Dieter Konrad
Cassidy James
Jaap Broekhuizen
Leonardo Lemos
Eze Sapo</property>
    <property name="artists">Dieter Konrad</property>
    <property name="logo">../media/background.png</property>
    <property name="license_type">mit-x11</property>
    <child internal-child="vbox">
      <object class="GtkBox" id="dialog-vbox1">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <child internal-child="action_area">
          <object class="GtkButtonBox" id="dialog-action_area">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="layout_style">end</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <placeholder/>
        </child>
      </object>
    </child>
  </object>
</interface>
